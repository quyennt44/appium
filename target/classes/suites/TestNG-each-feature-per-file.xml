<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
 
<suite name="Wordpress-Android-Test-Suite" parallel="tests" thread-count="5">
 
   <test name="Test-SamsungJ3" parallel="classes" thread-count="5">
        <parameter name="deviceName" value="SamsungJ3" />
        <parameter name="platformVersion" value="5.1.1" />
        <classes>
            <class name="vn.teko.appium.cucumber.test.runners.RunSignupFeature" />
        </classes>
    </test>
 
   <test name="Test-AndroidPad" parallel="classes" thread-count="5">
        <parameter name="deviceName" value="AndroidPad" />
        <parameter name="platformVersion" value="7.0" />
        <classes>
            <class name="vn.teko.appium.cucumber.test.runners.RunLoginFeature" />
        </classes>
    </test>
 
</suite>