<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
 
<suite name="Wordpress-Android-Test-Suite" parallel="tests" thread-count="2">
 
   <test name="Test-SamsungJ3" parallel="classes" thread-count="1">
        <parameter name="deviceName" value="SamsungJ3" />
        <parameter name="platformVersion" value="5.1.1" />
        <parameter name="udid" value="42006fd2cc118400" />
        <classes>
            <class name="vn.teko.appium.cucumber.test.runners.signup.RunSignupFeature" />
        </classes>
    </test>
 
   <test name="Test-AndroidPad" parallel="classes" thread-count="1">
        <parameter name="deviceName" value="AndroidPad" />
        <parameter name="platformVersion" value="5.1.1" />
        <parameter name="udid" value="5200e90dfa179400" />
        <classes>
            <class name="vn.teko.appium.cucumber.test.runners.login.RunLoginFeature" />
        </classes>
    </test> 
</suite>

